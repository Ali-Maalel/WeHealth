{% extends 'base.html.twig' %}



{% block body %}
<link rel="stylesheet" href="{{ asset('assets/css/signupform.css') }}">
<div class="signupform">
    {{ form_start(form) }}
    <div class="login_form">{{form_row(form.login)}}</div>
    <div class="password_form">{{form_row(form.password)}}</div>
    <div class="nom_form">{{form_row(form.nom)}}</div>
    <div class="prenom_form">{{form_row(form.prenom)}}</div>
    <div class="email_form">{{form_row(form.email)}}</div>
    <div class="telephone_form">{{form_row(form.telephone)}}</div>
    <div class="typeuser_form">{{form_rest(form)}}</div>
    
    {{ form_end(form) }}
</div>
{% endblock %}
